# Firebase Configuration
# Get these from your Firebase project settings
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Google Maps API
# Get an API key from Google Cloud Console with Maps JavaScript API enabled
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Map ID for Advanced Markers (required)
# Create a Map ID in Google Cloud Console → Maps → Map Management
VITE_GOOGLE_MAPS_MAP_ID=9846f743a351d898
